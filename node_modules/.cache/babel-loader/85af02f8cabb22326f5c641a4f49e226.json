\n          <v-btn color=\"primary\" dark class=\"mb-2\" @click=\"hour = false\">\r\n            close\r\n          </v-btn>\r\n        </template>\r\n      </v-data-table>\r\n    </v-card>\r\n    <v-card v-if=\"day == true\">\r\n      <v-data-table\r\n      :headers=\"headers\"\r\n      :items=\"desserts\"\r\n      :items-per-page=\"5\"\r\n      class=\"elevation-1\"\r\n      >\r\n        <template v-slot:top>\r\n          <v-spacer></v-spacer>\r\n          <v-btn color=\"primary\" dark class=\"mb-2\" @click=\"day = false\">\r\n            close\r\n          </v-btn>\r\n        </template>\r\n      </v-data-table>\r\n    </v-card>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\n\r\nexport default {\r\n  data: () => ({\r\n    city: \"Hà nội\",\r\n    NhietDo: 30,\r\n    rain: 50,\r\n    wind: 30,\r\n    cloud: false,\r\n    humidity: 60,\r\n    hour: false,\r\n    day: false,\r\n    desserts1: [\r\n      {\r\n        time: \"1 am\",\r\n        temperature: 159,\r\n        humidity: 6.0,\r\n        wind: 24,\r\n        chance_rain: 4.0,\r\n      },\r\n      {\r\n        time: \"2 am\",\r\n        temperature: 237,\r\n        humidity: 9.0,\r\n        wind: 37,\r\n        chance_rain: 4.3,\r\n      },\r\n      {\r\n        time: \"3 am\",\r\n        temperature: 262,\r\n        humidity: 16.0,\r\n        wind: 23,\r\n        chance_rain: 6.0,\r\n      },\r\n      {\r\n        time: \"4 am\",\r\n        temperature: 305,\r\n        humidity: 3.7,\r\n        wind: 67,\r\n        chance_rain: 4.3,\r\n      },\r\n      {\r\n        time: \"5 am\",\r\n        temperature: 356,\r\n        humidity: 16.0,\r\n        wind: 49,\r\n        chance_rain: 3.9,\r\n      },\r\n      {\r\n        time: \"6 am\",\r\n        temperature: 375,\r\n        humidity: 0.0,\r\n        wind: 94,\r\n        chance_rain: 0.0,\r\n      },\r\n      {\r\n        time: \"7 am\",\r\n        temperature: 392,\r\n        humidity: 0.2,\r\n        wind: 98,\r\n        